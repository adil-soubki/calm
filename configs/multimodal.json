{
    "text_model_name_or_path": "google/flan-t5-base",
    "audio_model_name_or_path": "facebook/wav2vec2-base",
    "use_opensmile_features": false,
    "output_dir": "outputs/cb/multimodal",
    "overwrite_output_dir": true,

    "save_total_limit": 1,
    "load_best_model_at_end": true,
    "metric_for_classification": "accuracy",
    "metric_for_regression": "mae",
    "save_strategy": "epoch",
    "evaluation_strategy": "epoch",

    "num_labels": 5,
    "data_seed": 19,
    "data_num_folds": 5,
    "do_regression": true,
    "do_train": true,
    "do_eval": true,

    "max_seq_length": 256,
    "per_device_train_batch_size": 1,
    "per_device_eval_batch_size": 1,
    "learning_rate": 2e-5,
    "num_train_epochs": 10,
    "save_safetensors": false
}
